{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],     
    "env": { 
    	"browser": true, 
    	"node": true 
    },
    "rules": {
    	"no-use-before-define": 0, 
    	"no-plusplus": 0,
    	"no-unused-vars": 0,
    	"func-names": 0,
        "space-before-function-paren": 0,
        "prefer-arrow-callback": 0,
        "no-shadow": 0,
        "object-shorthand": 0,
        "brace-style": 0,
        "comma-dangle": 0
    }

}